    "@types/jsonwebtoken": "^8.5.0",
    "@types/supertest": "^2.0.10",
    "@types/validator": "^13.1.1",
    "jest": "^26.4.2",
    "nodemon": "^2.0.4",
    "supertest": "^5.0.0",
    "ts-jest": "^26.4.0",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.3"
  },
  "dependencies": {
    "bcrypt": "^5.0.0",
    "dotenv-safe": "^8.2.0",
    "express": "^4.17.1",
